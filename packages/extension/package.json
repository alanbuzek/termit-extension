{
  "name": "my-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@hypothesis/frontend-shared": "^4.5.0",
    "@medv/finder": "^2.1.0",
    "@opendata-mvcr/assembly-line-shared": "^0.2.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-toggle": "^4.0.3",
    "approx-string-match": "^2.0.0",
    "axios": "^0.24.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.1.0",
    "classnames": "^2.3.1",
    "css-loader": "^3.6.0",
    "css-selector-generator": "^3.6.0",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "formik": "^2.2.9",
    "fs-extra": "^9.0.1",
    "hammerjs": "^2.0.8",
    "intelligent-tree-select": "^0.8.3",
    "jsdom": "^18.1.0",
    "jsonld": "^5.2.0",
    "lodash": "^4.17.21",
    "mark.js": "https://github.com/alanbuzek/mark.js",
    "optimal-select": "^4.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "preact": "^10.6.5",
    "prettier": "^2.3.1",
    "promise": "8.0.1",
    "prop-types": "^15.7.2",
    "raf": "3.4.1",
    "react": "^17.0.2",
    "react-bootstrap-toggle": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-dropdown": "^1.10.0",
    "react-dropdown-select": "^4.8.4",
    "react-frame-component": "^5.1.0",
    "react-highlight-words": "^0.18.0",
    "react-icons": "^4.3.1",
    "react-intl": "^5.24.6",
    "react-select": "^4.3.1",
    "react-toggle": "^4.1.2",
    "reactstrap": "^9.0.1",
    "regenerator-runtime": "^0.13.9",
    "resolve": "^1.17.0",
    "scroll-into-view": "^1.16.0",
    "tailwindcss": "^3.0.23",
    "tiny-emitter": "^2.1.0",
    "typescript": "^4.3.2",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "watch": "npm-watch",
    "build:css": "NODE_ENV=production tailwind build -i src/styles/index.css -o public/static/css/styles.css --minify",
    "watch:css": "npx tailwindcss -i src/styles/index.css -o public/static/css/styles.css --watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public/*.*",
        "public/static/css/content.css",
        "public/static/css/*.css"
      ],
      "extensions": "js,html,scss,css,json,svg,ts,tsx"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/chrome": "^0.0.145",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-watch": "^0.6.0",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^10.2.1",
    "sass-loader": "^10.0.3",
    "style-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "^1.0.0",
    "ts-loader": "^9.2.3",
    "url-loader": "^4.1.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
